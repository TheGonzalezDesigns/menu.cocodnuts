{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 8
	},
	"rules": {
		"indent": [
            "error",
            "tab"
        ],
		"linebreak-style": [
            "error",
            "unix"
        ],
		"quotes": [
            "error",
            "single"
        ],
		"semi": [
            "error",
            "never"
        ],
		"no-unused-vars": 1,
		"no-console": 1
	}
}
